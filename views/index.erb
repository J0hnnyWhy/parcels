<h1>Shipping Service App</h1>
<form action="/results">
  <div class="form-group">
    <label for ="length">Package Length</label>
    <input id="length" name="length" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="height">Package Width</label>
    <input id="height" name="height" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="depth">Package Depth</label>
    <input id="depth" name="depth" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="weight">Package Weight</label>
    <input id="weight" name="weight" class="form-control" type="text">
  </div>
  <button type="submit" class="btn-success">Send</button>
</form>
